                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "package": "com.stubbypigeon.Counter"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["expo-router"],
        "extra": {
            "router": {},
            "eas": {
                "projectId": "02cfb5ca-acc7-45ba-8518-451cb0935cce"
            }
        }
